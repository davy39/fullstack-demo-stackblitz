{
  "name": "simple-vite-react-express",
  "version": "1.0.0",
  "description": "Boilerplate to build fullstack app using react, express, vite and pg",
  "main": "src/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "tsc -b && vite build",
    "start": "npm run build && tsx src/server/index.ts",
    "server": "tsx watch --tsconfig src/server/tsconfig.json src/server/index.ts",
    "client": "vite",
    "preview": "vite preview",
    "db:setup": "npx prisma migrate dev && npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "tsx --tsconfig src/server/tsconfig.json prisma/seed.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc -b",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Avi Maiti",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@prisma/adapter-better-sqlite3": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.6",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "formik": "^2.4.9",
    "fs-extra": "^11.3.2",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "prisma": "^7.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "react-toastify": "^11.0.5",
    "strong-error-handler": "^5.0.25",
    "yup": "^1.7.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.6.2",
    "terser": "^5.44.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4"
  }
}
